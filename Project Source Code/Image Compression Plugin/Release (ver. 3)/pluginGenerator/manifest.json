{
    "manifest_version": 1,
    "version": 3.0,
    "type": "APP",
    "name": {
        "en": "OGUSU Image Compression Plugin"
    },
    "description": {
        "en": "This plugin (V 3.0.0) helps to compress images. It makes resizing and compressing images quick and simple. "
    },
    "icon": "bIcon1.png",
    "desktop": {
        "js": [
            "desktop.js",
            "lib/pica.min.js",
            "lib/sweetalert.min.js"
        ],
        "css": [
            "desktop.css",
            "lib/css/bootstrap.min.css",
            "lib/css/fontawesome.min.css"
        ]
    },
    "config": {
    "html": "config/config.html",
    "js": [
      "config/config.js"
    ],
    "css": [
      "config/51-modern-default.css",
      "config/config.css"
    ],
    "required_params": ["appApiKey"]
    }
}
